mixin range-slider
  div.range-slider
    h3.range-slider__title Диапазон цены
      output.range-slider__range(form="price" name="price-range")
    div.range-slider__plugin(data-on-range="true", data-min-value="0", data-max-value="15000", data-value-from="5000", data-value-to="10000")
    p.range-slider__info Стоимость за сутки пребывания в номере